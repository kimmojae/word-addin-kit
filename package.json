{
  "name": "vue3-starter-template",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "office-addin-dev-settings register manifest.xml && run-p dev:vite dev:word:mac",
    "dev:vite": "vite",
    "dev:word:mac": "sleep 3 && open -a 'Microsoft Word'",
    "dev:word:windows": "timeout /t 3 /nobreak && start winword",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix --cache",
    "format": "prettier --write --experimental-cli src/"
  },
  "dependencies": {
    "@iconify/vue": "^5.0.0",
    "@tanstack/vue-query": "^5.92.0",
    "@vueuse/core": "^14.1.0",
    "axios": "^1.13.2",
    "date-fns": "^4.1.0",
    "lucide-vue-next": "^0.555.0",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "pretendard": "^1.3.9",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/vue-query-devtools": "^6.1.2",
    "@tsconfig/node24": "^24.0.3",
    "@types/node": "^24.10.1",
    "@types/office-js": "^1.0.561",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "~10.5.1",
    "jiti": "^2.6.1",
    "npm-run-all2": "^8.0.4",
    "office-addin-dev-settings": "^3.0.6",
    "prettier": "3.6.2",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.9.0",
    "unplugin-auto-import": "^20.3.0",
    "unplugin-vue-components": "^30.0.0",
    "unplugin-vue-router": "^0.17.2",
    "vite": "^7.2.4",
    "vite-plugin-mkcert": "^1.17.9",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vue-tsc": "^3.1.5"
  }
}
